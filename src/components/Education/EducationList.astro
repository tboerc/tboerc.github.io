---
import type { CollectionEntry } from 'astro:content'
import Educationtem from '@components/Education/Educationtem.astro'

type Props = {
	collection: CollectionEntry<'education'>[]
}

const { collection } = Astro.props
---

<ul class="flex flex-col gap-6">{collection.map(({ data }) => <Educationtem {...data} />)}</ul>
